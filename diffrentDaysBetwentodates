function daysRemaining(startDate,endDate){
  const onedaySeconds=24*60*60*1000;
  const diffrentDays = Math.round(endDate -startDate  ) / onedaySeconds
  return diffrentDays
  
}


const today = new Date();

const threeYearsLater = new Date()
threeYearsLater.setFullYear(today.getFullYear() + 1)

console.log(daysRemaining(today,threeYearsLater))
